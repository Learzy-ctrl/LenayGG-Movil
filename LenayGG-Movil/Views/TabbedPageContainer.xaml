<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LenayGG_Movil.Views.TabbedPageContainer"
             xmlns:Main="clr-namespace:LenayGG_Movil.Views.Main"
             xmlns:Wallet="clr-namespace:LenayGG_Movil.Views.Wallet"
             xmlns:Notification="clr-namespace:LenayGG_Movil.Views.Notification"
             xmlns:Tool="clr-namespace:LenayGG_Movil.Views.Tools"
             Title="TabbedPageContainer"
             BarTextColor="White"
             BarBackgroundColor="Green"
             SelectedTabColor="Cyan"
             UnselectedTabColor="White"
             NavigationPage.HasNavigationBar="False">

    <Main:Main Title="Inicio"
               IconImageSource="home"/>

    <Wallet:Wallets Title="Billeteras"
                    IconImageSource="wallet"/>

    <Notification:Notifications Title="Notificaciones"
                                IconImageSource="ringing"/>
    <Tool:Tools Title="Herramientas"
                IconImageSource="support"/>



</TabbedPage>   